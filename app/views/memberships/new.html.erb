<h1>New membership</h1>

<p id="notice"><%= notice %></p>

<%= form_for (@membership) do |m| %>
    <%= m.select :beer_club_id, options_from_collection_for_select(BeerClub.all, :id, :to_s) %>
    <% m.hidden_field :user_id, :value => current_user.id %>
    <%= m.submit %>
<% end %>

<%= link_to 'Back', memberships_path %>
